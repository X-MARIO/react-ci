{
  "name": "@react-ci/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "node ./node_modules/nx/bin/nx",
    "ng": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx",
    "*** react-ci ***": "",
    "run": "node ./node_modules/nx/bin/nx run react-ci:start:development --no-cloud",
    "react-ci:start:production": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci:start:production --no-cloud",
    "react-ci:build:development": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci:build:development --no-cloud",
    "build": "nx run react-ci:build:production --no-cloud",
    "react-ci:lint:fix": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci:lint --fix=true --no-cloud",
    "react-ci-e2e:lint:fix": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci-e2e:lint --fix=true --no-cloud",
    "react-ci:test": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci:test",
    "react-ci:test-e2e:development": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run react-ci-e2e:e2e:development --no-cloud",
    "test": "nx run react-ci-e2e:e2e:production --no-cloud",
    "*** config ***": "",
    "nx:update": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx migrate latest --no-cloud",
    "nx:run-migrations": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx migrate --run-migrations --no-cloud",
    "ncu:update": "ncu -u -t minor",
    "ncu:update:interactive": "ncu -i --format group",
    "npm:audit": "npm audit",
    "npm:audit:fix": "npm audit fix --force",
    "lint": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --all --target=lint --no-cloud",
    "lint:fix": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --all --target=lint --fix=true --no-cloud",
    "stylelint": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint.mjs apps/**/*.scss --config .stylelintrc",
    "stylelint:fix": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint.mjs apps/**/*.scss --fix=true --config .stylelintrc",
    "test:all": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --all --target=lint --no-cloud"
  },
  "private": true,
  "dependencies": {
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.38.0",
    "@next/eslint-plugin-next": "16.0.1",
    "@nx/devkit": "22.0.2",
    "@nx/eslint": "22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/jest": "22.0.2",
    "@nx/js": "22.0.2",
    "@nx/next": "22.0.2",
    "@nx/playwright": "22.0.2",
    "@nx/workspace": "22.0.2",
    "@playwright/test": "1.56.1",
    "@swc-node/register": "1.11.1",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.14.0",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/postcss": "4.1.16",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "10.4.21",
    "babel-jest": "30.2.0",
    "eslint": "9.38.0",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-util": "30.2.0",
    "npm-check-updates": "19.1.2",
    "nx": "22.0.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.16",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  },
